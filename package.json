{
  "name": "vite-mui-ts",
  "version": "1.0.0",
  "description": "This is the frontend to www.nepaccess.org's document search functionality",
  "license": "MIT",
  "author": "georgeschaffer@email.arizona.edu",
  "keywords": [
    "NEPA",
    "EPA"
  ],
  "proxy": "http://localhost:8080/",
  "repository": {
    "type": "git",
    "url": "https://github.com/NEPAccessProject/nepaccessFront.git"
  },
  "bugs": {
    "url": "https://github.com/NEPAccessProject/nepaccessFront/issues"
  },
  "homepage": "https://www.nepaccess.org",
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --open --host",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run --record'",
    "dev:server": "json-server --watch ./src/data/data.json --port 8080 && vite --open"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "@mui/types": "^7.2.13",
    "@mui/x-date-pickers": "^6.19.3",
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.7",
    "debug": "^4.3.4",
    "dotenv": "^16.4.1",
    "http-proxy-middleware": "^2.0.6",
    "json-server": "^1.0.0-alpha.23",
    "loadash": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "styled-components": "^6.1.8",
    "ts-node": "^10.9.2",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@material-ui/core": "^4.12.4",
    "@mui/styles": "^5.15.9",
    "@types/async": "^3.2.24",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8.56.2",
    "@types/geojson": "^7946.0.14",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/material-ui": "^0.21.16",
    "@types/node": "^20.11.16",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/request": "^2.48.12",
    "@types/styled-components": "^5.1.34",
    "@types/validator": "^13.11.9",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "4.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  }
}
